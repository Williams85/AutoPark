<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.upc.autoparqueo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".actividades.ActualizarEstacionamientoActivity"></activity>
        <activity
            android:name=".actividades.ReservarEstacionamientoActivity"
            android:label="Reservar Estacionamiento"
            android:screenOrientation="portrait" />
        <activity
            android:name=".actividades.ActualizarReservaActivity"
            android:label="Actualizar Reserva"
            android:screenOrientation="portrait" />
        <activity
            android:name=".actividades.MenuActivity"
            android:label="@string/title_activity_menu"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".actividades.ActualizarUsuarioActivity"
            android:label="Actualizar Usuario"
            android:screenOrientation="portrait" />
        <activity
            android:name=".actividades.RegistrarUsuarioActivity"
            android:label="Registrar Usuario"
            android:screenOrientation="portrait" />
        <activity
            android:name=".actividades.InicioSesionActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>